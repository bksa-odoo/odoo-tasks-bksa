<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form_stock_transport_bridge" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.stock.transport</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//block[@name='shipping_setting_container']" position="inside">
                <setting id="stock_transports_dispatch" company_dependent="1" help="Auto Download the Transport Management App">
                    <field name='module_stock_transport'/>
                </setting>
            </xpath>
        </field>
    </record>
</odoo>